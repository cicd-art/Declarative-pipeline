pipeline {
    agent any
    
    
    environment {
        PATH = "/maven/bin:$PATH"
    }
    
    stages {
        stage("Git Checkout")
        {
            steps{
                git 'https://github.com/Sachiniraveni/Sachiniraveni.git'
            }
        }

        stage ("maven build")
        {
            steps {
                sh "sudo cd /root/mvn_jenkins/github-maven-example/example"
                sh "mvn clean package"
            }
        }
    }
}
