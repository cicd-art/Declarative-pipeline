pipeline {
    agent any
    stages {
        stage("Git Checkout")
        {
            steps{
                git 'https://github.com/Sachiniraveni/Sachiniraveni.git'
            }
        }

        stage ("maven build")
        {
            steps {
                sh "cd /root/mvn_jenkins/github-maven-example/example"
                sh "mvn clean package"
            }
        }
    }
}
